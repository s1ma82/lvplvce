{
  "name": "lvplvce",
  "private": true,
  "version": "0.1",
  "type": "module",
  "scripts": {
    "pack-orig": "google-chrome --pack-extension=./lvplvce --pack-extension-key=./privateKey.pem",
    "pack": "crx3 lvplvce --key ./privateKey.pem",
    "test": "echo ",
    "prebuild": "node ./prebuild.js",
    "release": "gh release create v$npm_package_version ./lvplvce.crx --notes",
    "dev": "vite",
    "build": "vite build --out-dir lvplvce",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --port 2223",
    "zip": "npm-build-zip --source=./lvplvce",
    "gen": "npx generate-react-cli component",
    "gen:ui": "npx generate-react-cli component --type=ui"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@vercel/analytics": "^1.1.2",
    "bootstrap-icons": "^1.11.1",
    "dedent": "^1.2.0",
    "dotenv": "^16.4.1",
    "is-url": "^1.2.4",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.11.2",
    "react-svg": "^16.1.22",
    "use-local-storage": "^3.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@tsconfig/vite-react": "^2.0.0",
    "@types/react": "^18.0.37",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@vitejs/plugin-react": "^4.0.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cross-env": "^7.0.3",
    "crx3": "^1.1.3",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "npm-build-zip": "^1.0.4",
    "sass": "^1.62.1",
    "terser": "^5.21.0",
    "typescript": "^5.1.3",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.3.9",
    "vite-plugin-compress": "^2.1.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-svgr": "^3.2.0"
  }
